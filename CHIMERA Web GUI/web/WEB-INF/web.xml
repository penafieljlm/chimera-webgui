<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ServletInitialize</servlet-name>
        <servlet-class>ph.edu.dlsu.chimera.gui.servlets.ServletInitialize</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFileHandler</servlet-name>
        <servlet-class>ph.edu.dlsu.chimera.gui.servlets.ServletFileHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletInitialize</servlet-name>
        <url-pattern>/ServletInitialize</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFileHandler</servlet-name>
        <url-pattern>/ServletFileHandler</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
